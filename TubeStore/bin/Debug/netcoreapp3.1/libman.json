{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "font-awesome@5.11.2",
      "destination": "wwwroot/lib/fontawesome/"
    },
    {
      "library": "admin-lte@3.0.2",
      "destination": "wwwroot/admin/lte/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.2",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/"
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "library": "bootstrap-sweetalert@1.0.1",
      "destination": "wwwroot/lib/bootstrap-sweetalert/"
    },
    {
      "library": "underscore.js@1.9.1",
      "destination": "wwwroot/lib/underscore.js/"
    }
  ]
}