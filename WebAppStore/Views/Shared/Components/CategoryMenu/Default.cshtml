@model IEnumerable<Category>

<li class="dropdown">
    <a asp-controller="Tube" asp-action="Index" class="dropdown-toggle" data-toggle="dropdown">Tubes</a>
    <ul class="dropdown-menu">
        @foreach(var category in Model)
        {
            <li>
                <a asp-controller="Tube" asp-action="List" asp-route-category="@category.CategoryName">
                    @category.CategoryName
                </a>
            </li>
        }
        <li class="dropdown-divider"></li>
        <li>
            <a asp-controller="Tube" asp-action="List" asp-route-Category="">View all tubes</a>
        </li>
    </ul>
</li>